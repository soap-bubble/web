{
  "name": "morpheus-electron",
  "version": "1.5.0",
  "description": "A minimal Electron application",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "start:watch": "cross-env NODE_ENV=development electromon .",
    "build": "babel src -d dist",
    "build:watch": "npm run build -- --watch",
    "start:dev": "npm run build && concurrently --kill-others \"npm run build -- --watch --skip-initial-build\" \"npm run start:watch\""
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^3.6.1",
    "electromon": "^1.0.10",
    "electron": "^1.8.4"
  },
  "dependencies": {
    "mkdirp": "^0.5.1",
    "path-to-regexp": "^2.2.1",
    "regenerator-runtime": "^0.12.0",
    "request": "^2.87.0"
  }
}
