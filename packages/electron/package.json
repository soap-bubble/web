{
  "name": "Morpheus",
  "version": "1.6.0",
  "description": "A minimal Electron application",
  "private": true,
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "start:watch": "cross-env NODE_ENV=development electromon .",
    "build": "babel src -d dist",
    "build:watch": "npm run build -- --watch",
    "start:dev": "npm run build && concurrently --kill-others \"npm run build -- --watch --skip-initial-build\" \"npm run start:watch\"",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Soapbubble Productions",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.6.1",
    "cross-env": "^5.2.0",
    "electromon": "^1.0.10",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "1.8.4"
  },
  "dependencies": {
    "electron-compile": "^6.4.3",
    "electron-squirrel-startup": "^1.0.0",
    "mkdirp": "^0.5.1",
    "path-to-regexp": "^2.2.1",
    "regenerator-runtime": "^0.12.0",
    "request": "^2.87.0"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "icon": "res/icon.ico"
      },
      "electronWinstallerConfig": {
        "name": "Morpheus",
        "author": "Soapbubble Productions",
        "iconUrl": "https://s3-us-west-2.amazonaws.com/soapbubble-morpheus-dev/icon.ico",
        "loadingGif": "res/globe.gif"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "Morpheus"
      }
    }
  }
}
