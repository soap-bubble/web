{
  "name": "@soapbubble/morpheus",
  "version": "1.4.1",
  "description": "",
  "main": "lib/index.js",
  "private": true,
  "scripts": {
    "test": "jest client",
    "test:play": "jest playthrough",
    "test:play:local": "cross-env NO_LAUNCH=true jest playthrough",
    "build:production": "npm run build:server && npm run build:client:production",
    "build:server": "babel server -d lib --copy-files",
    "build:client": "webpack --env",
    "build:client:production": "webpack --env.production=true",
    "build:client:staging": "webpack --env.staging=true",
    "build:client:watch": "webpack --env --watch",
    "gulp": "gulp",
    "lint": "eslint client config server",
    "db:update": "gulp db:update | bunyan",
    "webpack": "webpack",
    "db:prime": "gulp db:prime | bunyan",
    "start": "npm run build:client:production & npm run build:server && node lib/index",
    "start:staging": "npm run build:client:staging & npm run build:server && node lib/index",
    "start:dev": "npm run build:server && concurrently --kill-others \"npm run build:client:watch\" \"npm run build:server -- --watch --skip-initial-build\" \"nodemon lib/index.js --watch lib | bunyan\""
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "collectCoverageFrom": [
      "client/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>//public"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/jestBabelProcessor.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
