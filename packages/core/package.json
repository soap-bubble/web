{
  "name": "@soapbubble/core",
  "version": "1.13.0",
  "description": "The root soapbubble.online site",
  "browserslist": "last 2 Chrome versions",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/CaptEmulation/soapbubble.online.git"
  },
  "main": "lib/server.js",
  "author": "Halt Hammerzeit",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:server && npm run build:client:production && npm run build:client:staging",
    "build:server": "babel server -d lib --copy-files",
    "build:client": "webpack --env.development=true",
    "build:client:production": "webpack --env.production=true",
    "build:client:firebase:production": "webpack --env.production=true --env.firebase=true",
    "build:client:staging": "webpack --env.staging=true",
    "build:client:watch": "webpack --env.development=true --watch",
    "start": "npm run build:client:production & npm run build:server && node lib/index",
    "start:firebase": "firebase serve -p 4060",
    "start:staging": "npm run build:client:staging & npm run build:server && node lib/index",
    "start:dev": "npm run build:server && concurrently --kill-others \"npm run start:firebase\" \"npm run build:client:watch\" \"npm run build:server -- --watch --skip-initial-build\" \"nodemon lib/index.js --watch lib | bunyan\""
  },
  "dependencies": {
    "@contentful/rich-text-from-markdown": "^13.0.0",
    "@madebyconnor/rich-text-to-jsx": "^1.0.0",
    "@respond-framework/rudy": "^0.1.1-test.3",
    "@soapbubble/components": "^1.13.0",
    "axios": "^0.16.2",
    "better-npm-run": "0.0.13",
    "bluebird": "^3.4.7",
    "bootstrap": "^4.2.1",
    "bootstrap-social": "^5.1.1",
    "bunyan": "^1.8.12",
    "classnames": "^2.2.6",
    "config": "^1.28.1",
    "contentful": "^7.1.0",
    "detect-node": "^2.0.3",
    "express": "^4.14.0",
    "font-awesome": "https://registry.npmjs.org/font-awesome/-/font-awesome-4.7.0.tgz",
    "fs-extra": "^2.0.0",
    "google-auth-library": "^0.11.0",
    "lodash": "^4.17.4",
    "npm-run-all": "^4.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "^16.8.6",
    "react-google-login": "^2.9.3",
    "react-redux": "^7.1.0",
    "react-router-bootstrap": "^0.23.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.1.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "rxjs": "6.5.2",
    "rxjs-compat": "^6.5.2",
    "service-builder": "^1.0.1",
    "socket.io-client": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@soapbubble/style": "^1.13.0",
    "babel-loader": "8.0.5",
    "babel-plugin-angularjs-annotate": "^0.9.0",
