<?xml version='1.0' encoding='utf-8'?>
<widget id="online.soapbubble.morpheus" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Morpheus</name>
    <description>
        Where dreams never die
    </description>
    <author email="morpheus.dev@soapbubble.online" href="https://soapbubble.online">
        Soapbubble Productions
    </author>
    <icon density="mdpi" height="72" platform="ios" src="../morpheus/client/image/icon/icon72.png" width="72" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">0000000000
                                                                                                                                                        <allow-intent href="market:*" />
        <icon density="ldpi" src="../morpheus/client/image/icon/icon36.png" />
        <icon density="mdpi" src="../morpheus/client/image/icon/icon48.png" />
        <icon density="hdpi" src="../morpheus/client/image/icon/icon72.png" />
        <icon density="xhdpi" src="../morpheus/client/image/icon/icon96.png" />
        <icon density="xxhdpi" src="../morpheus/client/image/icon/icon144.png" />
        <icon density="xxxhdpi" src="../morpheus/client/image/icon/icon192.png" />
</platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="Orientation" value="landscape" />
    <preference name="Fullscreen" value="true" />
    <preference name="DisallowOverscroll" value="true" />
    <access origin="https://auth.soapbubble.online" subdomains="true" />
    <access origin="https://morpheus.soapbubble.online" subdomains="true" />
    <access origin="https://auth.soapbubble.online" subdomains="true" />
    <access origin="https://s3-us-west-2.amazonaws.com/soapbubble-morpheus-dev" subdomains="true" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <engine name="ios" spec="^4.5.5" />
    <engine name="android" spec="^7.0.0" />
    <engine name="browser" spec="^5.0.4" />
</widget>
